{
	"env": {
		"es6": true,
		"node": true
	},
	"extends": "standard",
	"globals": {
		"Atomics": "readonly",
		"SharedArrayBuffer": "readonly"
	},
	"parserOptions": {
		"ecmaFeatures": {
			"jsx": true
		},
		"ecmaVersion": 2018,
		"sourceType": "module"
	},
	"plugins": [
		"react"
	],
	"rules": {
		"no-tabs": "off",
		"indent": [
			"error",
			"tab",
			{
				"SwitchCase": 1
			}
		]
	}
}